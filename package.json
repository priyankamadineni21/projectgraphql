{
  "name": "projectql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/datasource.ts",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/datasource.ts migration:generate ./src/migration/$npm_config_name",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/argon2": "^0.15.0",
    "@types/jsonwebtoken": "^9.0.3",
    "apollo-server": "^3.12.1",
    "argon2": "^0.31.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "migrations": "^1.3.0",
    "nexus": "^1.3.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "tsconfig-paths": "^4.2.0"
  }
}
